FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "-1";
    object      icoFoam2@-1@0@U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    steetWalls
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    branchWalls
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    westStreet
    {
        type            zeroGradient;
    }
    eastStreet
    {
        type            fixedValue;
        value           uniform (-1 0 0);
    }
    northBranch
    {
        type            zeroGradient;
    }
    defaultFaces
    {
        type            empty;
    }
}


